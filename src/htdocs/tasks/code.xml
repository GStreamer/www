<?xml-stylesheet href="tasks.xsl" type="text/xsl"?>
<!DOCTYPE xsl:stylesheet
[
  <!ENTITY % gst-entities SYSTEM "../entities.gst">
  %gst-entities;
]>
<page>
  <title>GStreamer: Tasks</title>
  <body>
  </body>
  
  <tasks title="Location of Code">
    <codes>
  
      <code name="acme">
        <description>acme (now merged in gnome control-center since 2.6)</description>
        <repo>GNOME SVN</repo>
        <path>acme</path>
      </code>
  
      <code name="gnome-mixer-applet">
        <description>GNOME's mixer applet</description>
        <repo>GNOME SVN</repo>
        <path>gnome-applets/mixer</path>
      </code>
  
      <code name="gnome-media-gst-mixer">
        <description>GNOME volume control</description>
        <repo>GNOME SVN</repo>
        <path>gnome-media/gst-mixer</path>
      </code>
      
      <code name="gnome-media-profiles">
        <description>GNOME Media profiles</description>
        <repo>GNOME SVN</repo>
        <path>gnome-media/profiles</path>
      </code>

      <code name="grecord">
        <description>GNOME Sound Recorder</description>
        <repo>GNOME SVN</repo>
        <path>gnome-media/grecord</path>
      </code>

      <code name="libgnome">
        <description>GNOME development libraries</description>
        <repo>GNOME SVN</repo>
        <path>gnome-libs</path>
      </code>

      <code name="libgnomeui">
	<description>GNOME UI development libraries</description>
	<repo>GNOME SVN</repo>
	<path>libgnomeui</path>
      </code>

      <code name="sound-juicer">
        <description>Sound Juicer CD-ripper</description>
        <repo>GNOME SVN</repo>
        <path>sound-juicer</path>
      </code>

      <code name="gnome-media-gmix">
	<description>Old GNOME volume control</description>
	<repo>GNOME SVN</repo>
	<path>gnome-media/gmix</path>
       </code>

      <code name="gnome-media-cdplayer">
	<description>GNOME CD player</description>
	<repo>GNOME SVN</repo>
	<path>gnome-media/gnome-cd</path>
      </code>

      <code name="gst-libs-gst-mixer">
        <description>GStreamer mixer interface API</description>
        <repo>GStreamer Git</repo>
        <path>gst-plugins/gst-libs/gst/mixer</path>
      </code>
  
      <code name="gst-media-info">
        <description>GStreamer media-info library</description>
        <repo>GStreamer Git</repo>
        <path>gst-plugins/gst-libs/gst/media-info</path>
      </code>
  
      <code name="gst-launch">
        <description>GStreamer gst-launch</description>
        <repo>GStreamer Git</repo>
        <path>gstreamer/tools/gst-launch.c</path>
      </code>
      <code name="tag.h">
	<description>GStreamer tagging header file</description>
	<repo>GStreamer Git</repo>
	<path>gst-plugins/gst-libs/gst/tag/</path>
      </code>
      <code name="vorbistagg">
	<description>Vorbisdec C file with vorbis tag implementation</description>
	<repo>GStreamer Git</repo>
	<path>gst-plugins/ext/vorbis/vorbisdec.c</path>
      </code>
      <code name="basicomega">
	<description>Basicomega C file.</description>
	<repo>GStreamer Git</repo>
	<path>gstreamer/gst/schedulers/gstbasicscheduler.c</path>
      </code>
      <code name="totem">
        <description>Totem SVN module in GNOME SVN.</description>
        <repo>GNOME SVN</repo>
        <path>totem/</path>
      </code>

      <code name="playbin">
        <description>Playbin element in GStreamer SVN.</description>
        <repo>GStreamer Git</repo>
        <path>gst-plugins/gst/playback/</path>
      </code>
      <code name="subparse">
        <description>Subparse element in GStreamer Git.</description>
        <repo>GStreamer Git</repo>
        <path>gst-plugins/gst/subparse/</path>
      </code>
      <code name="website">
        <description>GStreamer website module in GStreamer Git.</description>
        <repo>GStreamer Git</repo>
        <path>www/</path>
      </code>
    </codes>
  
    <repos>
      <repo of="GNOME SVN">
        <web>http://svn.gnome.org/viewvc/</web>
        <anon>gnome svn anon</anon>
      </repo>
      <repo of="GStreamer Git">
        <web>&gst-repo-http;</web>
        <anon>gst git anon</anon>
      </repo>
    </repos>
  
  </tasks>
</page>
